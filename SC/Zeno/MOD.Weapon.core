public @IconUpdate ( Client, Level )
{
	if( Level == 4 )
		replaceClientICON( Client , C_ICON_JUBEIKAME, ICON_KAMEHAMEHA );
	if( Level == 5 )
		replaceClientICON( Client , C_ICON_SUKAME, ICON_KAMEHAMEHA );
	else if( Level == 3 )
		replaceClientICON( Client , C_ICON_SUKAME, ICON_KAMEHAMEHA );
}
public @BaseWeaponCreation ( Client, BaseWeapon, const Name[] )
{
	if ( equal( Name, "weapon_kamehameha" ) && getClientLEVEL( Client ) > 3 )
	{
		setBeamChargeTime( BaseWeapon, 25 );
		setBeamChargeKi( BaseWeapon, 35 );
	}
	if ( equal( Name, "weapon_kamehameha" ) && getClientLEVEL( Client ) > 4 )
	{
		setBeamChargeTime( BaseWeapon, 25 );
		setBeamChargeKi( BaseWeapon, 35 );
	}
	if ( equal( Name, "weapon_masenko" ) && getClientLEVEL( Client ) == 0 )
	{
		setBeamSpeed( BaseWeapon, 1800 );
		setBeamChargeKi( BaseWeapon, 30 );
	}
	
}
