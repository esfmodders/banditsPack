		set_msg_arg_int( 3, ARG_SHORT, SPR_JUBKAME_S );
		set_msg_arg_int( 5, ARG_SHORT, SPR_JUBKAME_T );
		set_msg_arg_int( 6, ARG_SHORT, SPR_JUBKAME_T );
		set_msg_arg_int( 4, ARG_SHORT, 20 );
